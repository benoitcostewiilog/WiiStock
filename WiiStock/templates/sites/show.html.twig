{% extends 'layout.html.twig' %}

{% block title %}Suppression site "{{ site.nom }}"{% endblock %}

{% block page_content %}
    <h1>SUPPRESSION SITE "{{ site.nom }}"</h1>

    <table class="table table-condensed table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>{{ site.id }}</td>
                <td>{{ site.nom }}</td>
            </tr>
        </tbody>
    </table>

    {{ include('sites/_delete_form.html.twig') }}
    
    <a href="{{ path ('parc_parametrage') }}" class="button btn-left pull-left">
        <span class="fa fa-angle-double-left"></span>Retour</a>
{% endblock %}
