{% extends 'base.html.twig' %}

{% block title %}Historiques{% endblock %}

{% block body %}
    <h1>Historiques</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ historique.id }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ historique.dateDebut ? historique.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ historique.dateFin ? historique.dateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Type_mouvement</th>
                <td>{{ historique.typeMouvement }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('historiques_index') }}">back to list</a>

    <a href="{{ path('historiques_edit', {'id': historique.id}) }}">edit</a>

    {{ include('historiques/_delete_form.html.twig') }}
{% endblock %}