<form method="post" action="{{ path('filiales_delete', {'id': filiale.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette filiale ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ filiale.id) }}">
    <button class="button btn-left pull-right">Supprimer</button>
</form>
