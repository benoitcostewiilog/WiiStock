{% extends 'layout.html.twig' %}

{% block title %}Suppression filiale "{{ filiale.nom }}"{% endblock %}

{% block page_content %}
    <h1>SUPPRESSION FILIALE "{{ filiale.nom }}"</h1>

    <table class="table table-condensed table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>{{ filiale.id }}</td>
                <td>{{ filiale.nom }}</td>
            </tr>
        </tbody>
    </table>

    {{ include('filiales/_delete_form.html.twig') }}

    <a href="{{ path ('parc_parametrage') }}" class="button btn-left pull-left">
        <span class="fa fa-angle-double-left"></span>Retour</a>
{% endblock %}