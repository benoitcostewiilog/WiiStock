<div class="row">
  
    {% if refArticleId is defined %}
        <div data-id='{{ refArticleId }}' data-target='#modalShowRefArticle' data-toggle='modal' class='col-3'
        onclick="showRow($(this), Routing.generate('reference_article_show', true), $('#modalShowRefArticle'))">
        <i class='fas fa-eye'></i>
        {% else %}
        <div data-id='{{ id }}' data-target='#modalShowArticle' data-toggle='modal' class='col-3'
        onclick="showRow($(this), Routing.generate('article_show', true), $('#modalShowArticle'))">
        <i class='fas fa-eye'></i>
        {% endif %}
    </div>