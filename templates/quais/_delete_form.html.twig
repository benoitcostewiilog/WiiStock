<form method="post" action="{{ path('quais_delete', {'id': quai.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer la réception ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ quai.id) }}">
    <button class="btn">Delete</button>
</form>