{% extends 'layout.html.twig' %}

{% block title %}ReferencesArticles{% endblock %}

{% block page_content %}
    <h1>ReferencesArticles</h1>
<div class="container">
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ references_article.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ references_article.libelle }}</td>
            </tr>
            <tr>
                <th>Photo_article</th>
                <td>{{ references_article.photoArticle }}</td>
            </tr>
        </tbody>
    </table>
    <a href="{{ path('references_articles_index') }}" class='button pull-left btn-left'>retour</a>
    {{ include('references_articles/_delete_form.html.twig') }}
    <a href="{{ path('references_articles_edit', {'id': references_article.id}) }}"class='button pull-right btn-left'>Modifier</a>
</div>
{% endblock %}