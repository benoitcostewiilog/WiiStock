<div data-id='{{ emplacementId }}' data-target='#modalEditEmplacement' data-toggle='modal' class='btn btn-xs btn-default command-edit'
    onclick="editRow($(this), Routing.generate('emplacement_edit_api', true), $('#modalEditEmplacement'), $('#submitEditEmplacement'))">
    <i class='fas fa-pencil-alt'></i>
</div>
<div class='btn btn-xs btn-default type-show'
    onclick="showRow($('#modalShowEmplacement'), $(this), Routing.generate('emplacement_show', true) )"
    data-id='{{ emplacementId }}' data-target='#modalShowEmplacement' data-toggle='modal'>
    <i class='fas fa-eye'></i>
</div>
<div class='btn btn-xs btn-default emplacement-delete'
    onclick="deleteRow($(this), $('#modalDeleteEmplacement'), $('#submitDeleteEmplacement'))" data-id='{{ emplacementId }}'
    data-target='#modalDeleteEmplacement' data-toggle='modal'>
    <i class='fas fa-trash'></i>
</div>

