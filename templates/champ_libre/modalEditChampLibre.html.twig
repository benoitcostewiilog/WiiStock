<form method=""
      class="modal fade"
      id="modalEditChampLibre"
      role="dialog"
      aria-labelledby="dataModalCenterTitle"
      aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered"
         role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"
                    id="exampleModalLongTitle">
                    Modifier un champ libre
                </h5>
                <button type="button"
                        class="close"
                        data-dismiss="modal"
                        aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body modal-input container-fluid"></div>
            <div class="error-msg p-4"></div>
            <div class="modal-footer">
                <button type="button"
                        class="btn"
                        data-dismiss="modal">
                    Annuler
                </button>
                <button type='button'
                        id="submitEditChampLibre"
                        class="btn btn-success data"
                        name='champLibre'
                        value=''>
                    Enregistrer
                </button>
            </div>
        </div>
    </div>
</form>
