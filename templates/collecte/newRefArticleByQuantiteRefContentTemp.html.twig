<div class="form-group col-12 mb-3">
    <label for="Articlefournisseur">Référence fournisseur</label>
    <select class="form-control data needed select2" name="articleFournisseur" id="articleFournisseurAdd">
        {% for ref in references %}
        <option value="{{ ref.id }}">{{ref.reference}}</option>
        {% endfor %}
    </select>
</div>
<div class="form-group col-12 mb-3">
    <label for="quantity">Quantité à collecter</label>
    <input class="form-control data needed" type="number" min=1 id="quantity" name="quantitie" value="0">
</div>
<div class="form-group col-12 mb-3">
    <label for="label">Libelle</label>
    <input class="form-control data needed" type="text" id="label" name="label" value="">
</div>