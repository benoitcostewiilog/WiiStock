<form method="" class="modal fade" id="modalBarcode" role="dialog" aria-labelledby="dataModalCenterTitle"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle"></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
           
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Annuler</button>
                <button id="submitBarcode" onclick="printJS({printable:  'barcode', type: 'html', maxWidth : 250});"
                    name='article' value='' type="button" data-dismiss="modal"
                    class="btn btn-primary btn-success data">Imprimer</button>
            </div>
        </div>
    </div>
</form>