{% extends 'layout.html.twig' %}

{% block title %}Exports de données{% endblock %}

{% block page_content %}

<h1>Exports de données (tâches très gourmandes en ressources serveurs et en temps) : </h1>
<br>

<div class="row justify-content-center">
    <label>References CEA</label>
</div>

<div class='row justify-content-center'>
    <div class="btn btn-primary" onclick='initExport($(this), "ref")'>
        <i class="fa fa-print mr-2"></i>Exporter au format CSV
    </div>
</div>

<hr>

<div class="row justify-content-center">
    <label>Articles</label>
</div>

<div class='row justify-content-center'>
    <div class="btn btn-primary" onclick='initExport($(this), "art")'>
        <i class="fa fa-print mr-2"></i>Exporter au format CSV
    </div>
</div>

<hr>

{% endblock %}
{% block javascript %}

<script src="{{ asset('js/exports.js') }}?{{ date().timestamp }}"></script>

{% endblock %}