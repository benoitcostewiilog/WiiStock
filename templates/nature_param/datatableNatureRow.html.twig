<div class="d-flex">
    <div>
        <div class="btn btn-col-action" data-id='{{ natureId }}' data-toggle='modal' data-target='#modalEditNature'
             onclick="clearErrorMsg($('#submitEditNature')), editRow($(this), Routing.generate('nature_api_edit', true), $('#modalEditNature'), $('#submitEditNature'))">
            <i class='fas fa-pencil-alt'></i>
        </div>
    </div>

    <div onclick="checkAndDeleteRow($(this), '#modalDeleteNature', 'nature_check_delete', '#submitDeleteNature')"
         data-id='{{ natureId }}' data-target='#modalDeleteNature' data-toggle='modal'>
        <div class="btn btn-col-action">
            <i class='fas fa-trash'></i>
        </div>
    </div>
</div>