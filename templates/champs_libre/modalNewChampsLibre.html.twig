<!-- Modal -->
<form method="" class="modal fade" id="modalNewChampsLibre" role="dialog" aria-labelledby="dataModalCenterTitle"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Créer un champs libre</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body modal-input container-fluid">
                <div class="form-group">
                    <label for="date-attendu">Label</label>
                    <input type="text" class="form-control data" name="label" value="" required="true">
                </div>
            </div>
            <div class="modal-body modal-input container-fluid">
                <div class="form-group">
                    <label for="typage">Typage</label>
                    <select class="form-control select2 data" name="typage" id='typage'>
                        <option value="booleen">Booléen</option>
                        <option value="number">Nombre</option>
                        <option value="text">Text</option>
                    </select>
                </div>
            </div>
            <div class="modal-body modal-input container-fluid">
                <div class="form-group">
                    <label for="date-attendu">Valeur par défaut</label>
                    <input type="text" class="form-control data" name="Valeur par défaut" value="" required="true">
                </div>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Annuler</button>
                <button id="submitChampsLibreNew" name='type' value='{{type.id}}' type="button" data-dismiss="modal"
                    class="btn btn-primary btn-success data">Enregistrer</button>
            </div>
        </div>
    </div>
</form>