<div class="row">
    <div data-id='{{ idType }}' data-toggle='modal' data-target='#modalEditType' class='col-2 champs-libre-edit'
        onclick="editRow($(this),  Routing.generate('type_edit_api', true), $('#modalEditType'), $('#submitEditType'))">
        <i class='fas fa-pencil-alt '></i>
    </div>
    <div class='col-2 type-delete' onclick="deleteRow($(this), $('#modalDeleteType'), $('#submitDeleteType'))"
        data-id='{{ idType }}' data-target='#modalDeleteType' data-toggle='modal'>
        <i class='fas fa-trash '></i>
    </div>
    <a href={{urlChampsLibre}} class='col-2 command-edit'><i class='fas fa-eye'></i></a>
</div>