<div class="form-group">
    <label for="date-attendu">Label</label>
    <input type="text" class="form-control data" name="label" value="{{champLibre.label}}" required="true">
</div>
<div class="form-group">
    <label for="typage">Typage</label>
    <select class="form-control select2 data" name="typage" id='typage'>
        <option value="{{champLibre.typage}}">{{champLibre.typage}}</option>
        {% if champLibre.typage != "booleen" %}
            <option value="booleen">Oui/Non</option>
        {% elseif champLibre.typage != "number" %}
            <option value="number">Nombre</option>
        {% elseif champLibre.typage != "text" %}
            <option value="text">Texte</option>
        {% endif %}
    </select>
</div>
<div class="form-group">
    <label for="date-attendu">Valeur par d√©faut</label>
    <input type="text" class="form-control data" name="valeur" value="{{champLibre.defaultValue}}" required="true">
</div>
</div>