<div class="d-flex">
    <div>
        <div class="btn btn-col-action" data-id='{{ typeId }}' data-toggle='modal' data-target='#modalEditType'
             onclick="editRow($(this), Routing.generate('types_api_edit', true), $('#modalEditType'), $('#submitEditType'))">
            <i class='fas fa-pencil-alt'></i>
        </div>
    </div>

    <div onclick="checkAndDeleteRow($(this), '#modalDeleteType', 'types_check_delete', '#submitDeleteType')"
         data-id='{{ typeId }}' data-target='#modalDeleteType' data-toggle='modal'>
        <div class="btn btn-col-action">
            <i class='fas fa-trash'></i>
        </div>
    </div>
</div>
