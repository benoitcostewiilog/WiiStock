<div class="form-group">
    <label for="dateStart">Date de début*</label>
    <input type="datetime-local" class="form-control data needed" name="dateStart" value="{{ urgence.dateStart|date('Y-m-d\\TH:i') }}">
</div>

<div class="form-group">
    <label for="dateEnd">Date de fin*</label>
    <input type="datetime-local" class="form-control data needed" name="dateEnd" value="{{ urgence.dateEnd|date('Y-m-d\\TH:i') }}">
</div>

<div class="form-group needed">
    <label for="source">Numéro de commande*</label>
    <input class="form-control data needed" name="commande" value="{{ urgence.commande }}">
</div>