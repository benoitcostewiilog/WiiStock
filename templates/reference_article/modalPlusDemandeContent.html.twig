        <label for="">Mes demandes</label>
        <select name="livraison" class="form-control ajax-autocomplete-article data select2" id="">
            {% for demande in demandes %}
            <option value="{{demande.id}}">
                {{demande.numero}}
            </option>
            {% endfor %}
        </select>
    </div>
    {{articleOrNo | raw }}
</div>