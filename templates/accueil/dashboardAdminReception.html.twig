<div class="{% if isDashboardExt %}row d-flex align-items-end{% endif %}">
    {% if isDashboardExt %}
        <div>
            <img src="{{ asset('img/followGTwhite.svg') }}" alt="Logo" height="100px" class="m-2" >
        </div>
    {% endif %}
    <div class="white mt-2 mb-2{% if isDashboardExt %} spec-font mb-4 ml-5{% endif %}">
        Dashboard | RÉCEPTION ADMINISTRATIVE
    </div>
</div>
<div class=" h-100 container-fluid ">
<div class="col-12 {% if isDashboardExt %} space {% endif %} p-0">
        <div class="row">
            <div class="col-9 dashboard-box-wrapper third-height">
                <div class="dashboard-box">
                    <p class=" title {% if isDashboardExt %} medium-font {% endif %}">
                        Entrées à effectuer
                        {% if isCurrentClient(constant('App\\Service\\SpecificService::CLIENT_SAFRAN_ED')) %}
                            Composant
                        {% endif %}
                    </p>
                    {% if not isDashboardExt %}
                        <div class="points has-tooltip"
                             title="Nombre {{ 'arrivage.de colis'|trans }} (de nature paramétrée pour le graphe 1)
                                présents depuis < 48h sur l'ensemble des emplacements paramétrés pour le graphe 1.">
                            <i class="fa fa-question"></i>
                        </div>
                    {% endif %}
                    <div class="h-100">
                        <canvas id="chartFirstForAdmin" width="300" height="90"></canvas>
                    </div>
                </div>
            </div>
            <div class="col-3 d-flex flex-column justify-content-between">
                <div class="mr-1">
                    <div class="dashboard-box-wrapper side-panels">
                        <div class="dashboard-box justify-content-between pb-2">
                            {% if not isDashboardExt %}
                                <div class="points">
                                    <i class="fa fa-question has-tooltip"
                                       title="Nombre {{ 'arrivage.de colis'|trans }} présents
                                        sur l'emplacement paramétré sur le graphe 1 ayant
                                        l'encours de nature paramétrée pour le graphe 1 le plus ancien.">
                                    </i>
                                </div>
                            {% endif %}
                            <p class="text-center medium-font bold ">
                                Nombre de lignes à traiter
                            </p>
                            <div class="d-flex align-items-center justify-content-center ellipsis"
                                 style="height: inherit;">
                                <p class="big-font">
                                    <strong id="totalForChart1" class="figure"></strong>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="mr-1">
                    <div class="dashboard-box-wrapper side-panels">
                        <div class="dashboard-box text-center justify-content-around">
                            {% if not isDashboardExt %}
                                <div class="points">
                                    <i class="fa fa-question has-tooltip"
                                       title="Emplacement paramétré sur le graphe 1
                                        ayant l'encours de nature pour le graphe 1 paramétrée le plus ancien.">
                                    </i>
                                </div>
                            {% endif %}
                            <p class="text-center medium-font bold ">
                                Prochain emplacement à traiter
                            </p>
                            <div class="d-flex align-items-center justify-content-center"
                                 style="height: inherit;">
                                <p class="display-font figure " id="empForChart1" >Emplacement 12345
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<div class="col-12 {% if isDashboardExt %} space {% endif %} p-0">
    <div class="row">
        <div class="col-9 dashboard-box-wrapper third-height">
            <div class="dashboard-box">
                <p class="title {% if isDashboardExt %} medium-font  {% endif %}">
                    Entrées à effectuer
                    {% if isCurrentClient(constant('App\\Service\\SpecificService::CLIENT_SAFRAN_ED')) %}
                        Méca
                    {% endif %}
                </p>
                {% if not isDashboardExt %}
                    <div class="points">
                        <i
                                class="fa fa-question has-tooltip"
                                title="Nombre {{ 'arrivage.de colis'|trans }} (de natures paramétrées pour le graphe 2)
                                présents depuis < 48h sur l'ensemble des emplacements paramétrés pour le graphe 2."></i>
                    </div>
                {% endif %}
                <div class="h-100">
                    <canvas id="chartSecondForAdmin" width="300" height="90"></canvas>
                </div>
            </div>
        </div>
        <div class="col-3 d-flex flex-column justify-content-between">
            <div class="mr-1">
                <div class="dashboard-box-wrapper side-panels">
                    <div class="dashboard-box">
                        {% if not isDashboardExt %}
                            <div class="points">
                                <i class="fa fa-question has-tooltip"
                                   title="Nombre {{ 'arrivage.de colis'|trans }} présents
                                        sur l'emplacement paramétré sur le graphe 2 ayant l'encours
                                        de nature paramétrée pour le graphe 2 le plus ancien.">
                                </i>
                            </div>
                        {% endif %}
                        <p class="text-center {{ isDashboardExt ? 'medium-font bold' }}">
                            Nombre de lignes à traiter
                        </p>
                        <div class="d-flex align-items-center justify-content-center ellipsis"
                             style="height: inherit;">
                            <p class="big-font">
                                <strong id="totalForChart2" class="figure"></strong>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="mr-1">
                <div class="dashboard-box-wrapper side-panels">
                    <div class="dashboard-box text-center">
                        {% if not isDashboardExt %}
                            <div class="points">
                                <i class="fa fa-question has-tooltip"
                                   title="Emplacement paramétré sur le graphe 2
                                        ayant l'encours de nature paramétrée pour le graphe 2 le plus ancien.">
                                </i>
                            </div>
                        {% endif %}
                        <p class="text-center {{ isDashboardExt ? 'medium-font bold' }}">
                            Prochain emplacement à traiter
                        </p>
                        <div class="d-flex align-items-center justify-content-center"
                             style="height: inherit;">
                            <p class="display-font figure " id="empForChart2">blabla
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="col-12 {% if isDashboardExt %} space {% endif %} p-0">
    <div class="row">
        <div class="col-3">
            <div class="dashboard-box-wrapper fourth-height justify-content-around">
                <div id="remaining-urgences-box-admin" class="dashboard-box text-center ">
                    <p class="text-center bold medium-font">
                        Urgences à recevoir
                    </p>
                    <div class="d-flex align-items-center justify-content-center"
                         style="height: inherit;">
                        <p class="big-font figure counter"></p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-3 ">
            <div class="dashboard-box-wrapper fourth-height">
                <div id="encours-urgence-box" class="dashboard-box text-center justify-content-between">
                    <div>
                    <p class="text-center bold medium-font">
                        <i class="fas "></i>
                        Urgences à traiter
                        <i class="fas "></i>
                    </p>
                        <div class="location-label ellipsis lh medium-font"></div>
                    </div>
                    <div class="align-items-center lh">
                        <p class="big-font figure counter"></p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-3 ">
            <div class="dashboard-box-wrapper fourth-height">
                <div id="encours-litige-box" class="dashboard-box text-center justify-content-between">
                    <div>
                        <p class="text-center bold medium-font">
                        Lignes en litiges
                        </p>
                        <div class="location-label ellipsis lh medium-font"></div>
                    </div>
                    <div class="align-items-center lh">
                        <p class="big-font figure counter"></p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-3">
            <div class="dashboard-box-wrapper fourth-height">
                <div id="encours-clearance-box-admin" class="dashboard-box text-center justify-content-between">
                    <div>
                    <p class="text-center bold medium-font">
                        Colis en attente de dédouanement
                    </p>
                        <div class="location-label ellipsis lh medium-font"></div>
                    </div>
                    <div class="align-items-center lh"
                         >
                        <p class="big-font figure counter"></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>