<div class="col-12">
    <div class="row">
        <div class="col-12 space">
            {% include 'accueil/dashboardTitle.html.twig' with {isDashboardExt: isDashboardExt, title: 'Dashboard | RÉCEPTION ADMINISTRATIVE'} %}
        </div>
        <div class="col-12 space">
            <div class="row">
                <div class="col-9 dashboard-box-wrapper third-height">
                    <div class="dashboard-box">
                        <p class="title bold {{ isDashboardExt ? 'ext-title-font' }}">
                            Entrées à effectuer
                            {% if isCurrentClient(constant('App\\Service\\SpecificService::CLIENT_SAFRAN_ED')) %}
                                Composant
                            {% endif %}
                        </p>
                        {% if not isDashboardExt %}
                            <div class="points has-tooltip"
                                 title="Nombre {{ 'arrivage.de colis'|trans }} (de nature paramétrée pour le graphe 1)
                            présents depuis < 48h sur l'ensemble des emplacements paramétrés pour le graphe 1.">
                                <i class="fa fa-question"></i>
                            </div>
                        {% endif %}
                        <div class="h-100">
                            <canvas id="chartFirstForAdmin" width="300" height="90"></canvas>
                        </div>
                    </div>
                </div>
                <div class="col-3 d-flex flex-column justify-content-between">
                    <div>
                        <div class="dashboard-box-wrapper side-panels">
                            <div class="dashboard-box justify-content-between">
                                {% if not isDashboardExt %}
                                    <div class="points">
                                        <i class="fa fa-question has-tooltip"
                                           title="Nombre {{ 'arrivage.de colis'|trans }} présents
                                    sur l'emplacement paramétré sur le graphe 1 ayant
                                    l'encours de nature paramétrée pour le graphe 1 le plus ancien.">
                                        </i>
                                    </div>
                                {% endif %}
                                <p class="text-center bold title {{ isDashboardExt ? 'ext-title-font pt-0' : 'medium-font' }}">
                                    Nombre de lignes à traiter
                                </p>
                                <div class="text-center mb-1 {{ isDashboardExt ? ' ' : 'mb-1' }}">
                                    <p class="big-font {{ isDashboardExt ? 'mb-5' : ' ' }}">
                                        <strong id="totalForChart1" class="figure"></strong>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="dashboard-box-wrapper side-panels">
                            <div class="dashboard-box text-center justify-content-between">
                                {% if not isDashboardExt %}
                                    <div class="points">
                                        <i class="fa fa-question has-tooltip"
                                           title="Emplacement paramétré sur le graphe 1
                                          ayant l'encours de nature pour le graphe 1 paramétrée le plus ancien.">
                                        </i>
                                    </div>
                                {% endif %}
                                <p class="text-center bold title {{ isDashboardExt ? 'ext-title-font pt-0' : 'medium-font' }}">
                                    Prochain emplacement à traiter
                                </p>
                                <div class="d-flex align-items-center justify-content-center"
                                     style="height: inherit;">
                                    <p class="display-font figure counter" id="empForChart1"></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-12 space">
            <div class="row">
                <div class="col-9 dashboard-box-wrapper third-height">
                    <div class="dashboard-box">
                        <p class="title bold {{ isDashboardExt ? 'ext-title-font' }}">
                            Entrées à effectuer
                            {% if isCurrentClient(constant('App\\Service\\SpecificService::CLIENT_SAFRAN_ED')) %}
                                Méca
                            {% endif %}
                        </p>
                        {% if not isDashboardExt %}
                            <div class="points">
                                <i
                                    class="fa fa-question has-tooltip"
                                    title="Nombre {{ 'arrivage.de colis'|trans }} (de natures paramétrées pour le graphe 2)
                            présents depuis < 48h sur l'ensemble des emplacements paramétrés pour le graphe 2.">

                                </i>
                            </div>
                        {% endif %}
                        <div class="h-100">
                            <canvas id="chartSecondForAdmin" width="300" height="90"></canvas>
                        </div>
                    </div>
                </div>
                <div class="col-3 d-flex flex-column justify-content-between">
                    <div>
                        <div class="dashboard-box-wrapper side-panels">
                            <div class="dashboard-box justify-content-between">
                                {% if not isDashboardExt %}
                                    <div class="points">
                                        <i class="fa fa-question has-tooltip"
                                           title="Nombre {{ 'arrivage.de colis'|trans }} présents
                                        sur l'emplacement paramétré sur le graphe 2 ayant l'encours
                                        de nature paramétrée pour le graphe 2 le plus ancien.">
                                        </i>
                                    </div>
                                {% endif %}
                                <p class="text-center bold title {{ isDashboardExt ? 'ext-title-font' : 'medium-font' }}">
                                    Nombre de lignes à traiter
                                </p>
                                <div class="text-center mb-1">
                                    <p class="big-font">
                                        <strong id="totalForChart2" class="figure"></strong>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="dashboard-box-wrapper side-panels">
                            <div class="dashboard-box text-center justify-content-between">
                                {% if not isDashboardExt %}
                                    <div class="points">
                                        <i class="fa fa-question has-tooltip"
                                           title="Emplacement paramétré sur le graphe 2
                                          ayant l'encours de nature paramétrée pour le graphe 2 le plus ancien.">
                                        </i>
                                    </div>
                                {% endif %}
                                <p class="text-center bold title {{ isDashboardExt ? 'ext-title-font' : 'medium-font' }}">
                                    Prochain emplacement à traiter
                                </p>
                                <div class="d-flex align-items-center justify-content-center"
                                     style="height: inherit;">
                                    <p class="display-font figure" id="empForChart2"></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-12 space">
            <div class="row">
                <div class="col-3">
                    <div class="dashboard-box-wrapper fourth-height">
                        <div id="remaining-urgences-box-admin" class="dashboard-box text-center justify-content-between">
                            <p class="text-center title {{ isDashboardExt ? 'ext-title-font' : 'medium-font' }} bold">
                                Urgences à recevoir
                            </p>
                            <div class="align-items-end justify-content-center">
                                <p class="big-font figure counter"></p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-3">
                    <div class="dashboard-box-wrapper fourth-height">
                        <div id="encours-urgence-box" class="dashboard-box text-center justify-content-between">
                            <div>
                                <p class="text-center bold title {{ isDashboardExt ? 'ext-title-font' : 'medium-font' }}">
                                    <i class="fas"></i>
                                    Urgences à traiter
                                    <i class="fas"></i>
                                </p>
                                <div class="location-label ellipsis medium-font"></div>
                            </div>
                            <div class="align-items-center">
                                <p class="big-font figure counter"></p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-3 ">
                    <div class="dashboard-box-wrapper fourth-height">
                        <div id="encours-litige-box" class="dashboard-box text-center justify-content-between">
                            <div>
                                <p class="text-center bold title {{ isDashboardExt ? 'ext-title-font' : 'medium-font' }}">
                                    Lignes en litiges
                                </p>
                                <div class="location-label ellipsis medium-font"></div>
                            </div>
                            <div class="align-items-center">
                                <p class="big-font figure counter"></p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-3">
                    <div class="dashboard-box-wrapper fourth-height">
                        <div id="encours-clearance-box-admin" class="dashboard-box text-center justify-content-between">
                            <div>
                                <p class="text-center bold title {{ isDashboardExt ? 'ext-title-font' : 'medium-font' }}">
                                    Colis en attente de dédouanement
                                </p>
                                <div class="location-label ellipsis medium-font"></div>
                            </div>
                            <div class="align-items-center">
                                <p class="big-font figure counter"></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
