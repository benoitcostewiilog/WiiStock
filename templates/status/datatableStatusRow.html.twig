<div class="d-flex">
    <div>
        <div class="btn btn-col-action" data-id='{{ statusId }}' data-toggle='modal' data-target='#modalEditStatus'
             onclick="clearErrorMsg($('#submitEditStatus')), editRow($(this), Routing.generate('status_api_edit', true), $('#modalEditStatus'), $('#submitEditStatus'))">
            <i class='fas fa-pencil-alt'></i>
        </div>
    </div>

    <div onclick="checkAndDeleteRow($(this), '#modalDeleteStatus', 'status_check_delete', '#submitDeleteStatus')"
         data-id='{{ statusId }}' data-target='#modalDeleteStatus' data-toggle='modal'>
        <div class="btn btn-col-action">
            <i class='fas fa-trash'></i>
        </div>
    </div>
</div>
