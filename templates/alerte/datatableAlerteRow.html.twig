<div class="row">
    <div class="col-3" data-id='{{ alerteId }}' data-target='#modalEditAlerte' data-toggle='modal'
        onclick="editRow($(this), Routing.generate('alerte_api_edit', true), $('#modalEditAlerte'), $('#submitEditAlerte'))">
        <i class='fas fa-pencil-alt'></i>
    </div>
    <div class="col-3" onclick="deleteRow($(this), $('#modalDeleteAlerte'), $('#submitDeleteAlerte'))"
        data-id='{{ alerteId }}' data-target='#modalDeleteAlerte' data-toggle='modal'>
        <i class='fas fa-trash'></i>
    </div>
</div>