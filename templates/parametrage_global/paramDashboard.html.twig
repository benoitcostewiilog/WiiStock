<a data-toggle="collapse" href="#paramDashboard" role="button" aria-expanded="false"
   aria-controls="paramReceptions" class="col-12 mb-4">
    <h1 class="btn btn-primary m-0 w-100 border-radius-light">Configuration tableaux de bord
        <i class="fas fa-arrow-alt-circle-down ml-2"></i></h1>
</a>
<div class="collapse col-12" id="paramDashboard">
    <div class="float-right btn btn-col-action btn-check-reverse" onclick="editDashboardParams()">
        <i class='fas fa-check'></i>
    </div>
    <div class="form-group w-fit-content">
        <h2 class="bold">Réception administrative</h2>
    </div>
    <div class="form-group w-fit-content">
        <label>Nature de colis 1er tableau</label>
        <select name="nature-colis" class="form-control data">
            {% for nature in natures %}
                {% set selected = nature.id == existingNatureId ? 'selected' : '' %}
                <option value="{{ nature.id }}" {{ selected }}>{{ nature.label }}</option>
            {% endfor %}
        </select>
    </div>

    <div class="form-group w-211px">
        <div>
            <label class="d-block">Natures de colis 2è tableau</label>
            <select name="list-natures-colis" class="form-control d-block select2 data" multiple>
                {% for nature in natures %}
                    {% set selected = nature.id in existingListNaturesId ? 'selected' : '' %}
                    <option value="{{ nature.id }}" {{ selected }}>{{ nature.label }}</option>
                {% endfor %}
            </select>
        </div>
    </div>

</div>