<div class="d-flex">
    {% if hasRight(constant('App\\Entity\\Menu::PARAM')) %}
        <div data-target='#modalEditAlerteStock' data-toggle='modal'>
            <div class="btn btn-col-action" data-id='{{ alerteId }}'
            onclick="editRow($(this), Routing.generate('alerte_stock_api_edit', true), $('#modalEditAlerteStock'), $('#submitEditAlerteStock'))">
                <i class='fas fa-pencil-alt'></i>
            </div>
        </div>
        <div>
            <div class="btn btn-col-action"
            onclick="deleteRow($(this), $('#modalDeleteAlerteStock'), $('#submitDeleteAlerteStock'))"
            data-id='{{ alerteId }}' data-target='#modalDeleteAlerteStock' data-toggle='modal'>
                <i class='fas fa-trash'></i>
            </div>
        </div>
    {% endif %}
</div>