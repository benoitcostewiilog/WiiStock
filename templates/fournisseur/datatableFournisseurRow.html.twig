<a href='{{ url.edit }}' data-toggle='modal' class='btn btn-xs btn-default command-edit'>
    <i class='fas fa-pencil-alt fa-2x'></i>
</a>
<div data-toggle='modal' data-target='#modalShowFournisseur' data-id='{{fournisseurId}}' class='btn btn-xs btn-default command-edit '>
    <i class='fas fa-eye fa-2x'></i>
</div>
<div class='btn btn-xs btn-default fournisseur-delete'
    onclick="deleteRow($(this), $('#modalDeleteFournisseur'), $('#submitDeleteFournisseur'))" data-id='{{ fournisseurId }}'
    data-target='#modalDeleteFournisseur' data-toggle='modal'>
    <i class='fas fa-trash fa-2x'></i>
</div>