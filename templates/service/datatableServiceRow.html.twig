<div class="d-flex">

    {% if hasRight(constant('App\\Entity\\Menu::MANUT'), constant('App\\Entity\\Action::LIST')) and service.statut.nom == constant('App\\Entity\\Service::STATUT_BROUILLON')%}
    <div data-id='{{ serviceId }}' data-target='#modalEditService' data-toggle='modal'
        class='btn-xs btn-default command-edit'
        onclick="editRow($(this), Routing.generate('service_edit_api', true), $('#modalEditService'), $('#submitEditService'), true)">
        <i class='fas fa-pencil-alt'></i>
    </div>
    <div data-id='{{ serviceId }}' data-target='#modalDeleteService' data-toggle='modal' class='service-delete col-3'
        onclick="deleteRow($(this), $('#modalDeleteService'), $('#submitDeleteService'))">
        <i class='fas fa-trash'></i>
    </div>
    {% endif %}
    
    {% if hasRight(constant('App\\Entity\\Menu::MANUT'), constant('App\\Entity\\Action::LIST')) and service.statut.nom != constant('App\\Entity\\Service::STATUT_BROUILLON')%}
    <div data-id='{{ serviceId }}' data-target='#modalShowService' data-toggle='modal'
        class='btn-xs btn-default'
        onclick="editRow($(this), Routing.generate('service_show', true), $('#modalShowService'), true)">
        <i class='fas fa-eye'></i>
    </div>
    

    {% endif %}
</div>